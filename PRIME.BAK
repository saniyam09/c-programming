#include<stdio.h>
int main()
{
int i,num,prime=1;
printf("enter a prime number");
scanf("%d",&num);
for(i=2; i<=num/2; i++){
  if(num%i==0){
  prime=0;
  break;
  }
  }
  if(prime && num > 1){
  printf("%d is a prime number\n",num);
  }else{
  printf("%d is not a prime number\n",num);
  }
  return 0;
  }